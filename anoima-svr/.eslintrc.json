{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 1,
        "no-var": 1,
        "indent": ["error", "tab"],
        "semi": ["error", "always"]
    }
}