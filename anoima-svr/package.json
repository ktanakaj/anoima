{
  "name": "anoima-svr",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "pm2 start process.json",
    "production": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "build": "npm run clean-app && tsc --outDir ./lib",
    "watch": "npm run clean-app && tsc --watch --outDir ./lib",
    "test": "NODE_ENV=test mocha --compilers ts:espower-typescript/guess --recursive ./test",
    "doc": "npm run clean-doc && typedoc ./src ./test --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./lib",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint ./src ./test"
  },
  "license": "MIT",
  "dependencies": {
    "accept-language-parser": "^1.2.0",
    "body-parser": "~1.15.1",
    "config": "^1.21.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "express": "~4.13.4",
    "js-yaml": "^3.6.1",
    "log4js": "^0.6.38",
    "mysql": "^2.11.1",
    "passport": "^0.3.2",
    "request": "^2.74.0",
    "request-promise": "^4.0.2",
    "sequelize": "^3.27.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/config": "0.0.31",
    "@types/cookie-parser": "^1.3.30",
    "@types/ejs": "^2.3.33",
    "@types/express": "^4.0.34",
    "@types/log4js": "0.0.32",
    "@types/node": "0.0.2",
    "@types/passport": "^0.2.34",
    "@types/request": "0.0.36",
    "@types/request-promise": "^4.1.33",
    "@types/sequelize": "^4.0.39",
    "@types/swagger-jsdoc": "0.0.1",
    "espower-typescript": "^6.0.1",
    "mocha": "^2.5.3",
    "pm2": "^1.1.3",
    "power-assert": "^1.4.1",
    "sqlite3": "^3.1.4",
    "swagger-jsdoc": "^1.6.0",
    "tslint": "^4.0.1",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.10"
  }
}
